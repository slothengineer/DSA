        "## time complexity O(nlog(n))\n",
        "\n",
        "def mergeprocedure(arr, i ,mid ,j):\n",
        "  n1= mid - i + 1\n",
        "  n2= j - (mid + 1) +1\n",
        "\n",
        "  lsubarray = [0] * n1\n",
        "  rsubarray = [0] * n2\n",
        "\n",
        "  for m in range(n1):\n",
        "    lsubarray[m] = arr[i + m]\n",
        "\n",
        "  for n in range(n2):\n",
        "    rsubarray[n] = arr[mid + 1 + n]\n",
        "\n",
        "    p = 0\n",
        "    q = 0\n",
        "    k = i\n",
        "\n",
        "  while p < n1 and q < n2:\n",
        "      if lsubarray[p] <= rsubarray[q]:\n",
        "        arr[k] = lsubarray[p]\n",
        "        p += 1\n",
        "      else:\n",
        "        arr[k] = rsubarray[q]\n",
        "        q += 1\n",
        "      k += 1\n",
        "\n",
        "  while p < n1:\n",
        "      arr[k] = lsubarray[p]\n",
        "      p += 1\n",
        "      k += 1\n",
        "\n",
        "  while q < n2:\n",
        "      arr[k] = rsubarray[q]\n",
        "      q += 1\n",
        "      k += 1\n",
        "\n",
        "\n",
        "\n",
        "def mergesort(arr,i,j):\n",
        "  if i < j:\n",
        "    mid=(i+(j-1)) // 2\n",
        "\n",
        "    mergesort(arr, i , mid)\n",
        "    mergesort(arr, mid+1 ,j)\n",
        "\n",
        "    mergeprocedure(arr,i,mid,j)\n",
        "  return arr\n",
        "\n",
        "arr = [64, 34, 25, 12, 22, 11, 90]\n",
        "mergesort(arr, 0, len(arr)-1)\n",
        "print(\"Sorted array:\", arr)"
