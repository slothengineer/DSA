        "## time complexity O(log(n))\n",
        "\n",
        "def binarysearch(arr, n, i, j):\n",
        "  i = 0\n",
        "  j = len(arr) - 1\n",
        "  while i < j:\n",
        "    mid = i + (j - 1) // 2\n",
        "    if arr[mid] == n:\n",
        "      return mid\n",
        "    elif n < arr[mid]:\n",
        "      j = mid - 1\n",
        "    else:\n",
        "      i = mid + 1\n",
        "  return -1\n",
        "\n",
        "\n",
        "arr = [1,11,14,22,24,29]\n",
        "n = 24\n",
        "i = 0\n",
        "j = len(arr) - 1\n",
        "print("Searched element:"binarysearch(arr, n, i, j))"
